    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb-mi",
            "request": "launch",
            "name": "LLDB",
            "target": "./test",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch Program",
            "target": "./test",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "Debug",
            "type": "gdb",
            "request": "launch",
            "target": "${workspaceFolder}/zig-cache/bin/run",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "Debug Windows",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/zig-cache/bin/run.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/zig-cache/bin",
            "symbolSearchPath": "${workspaceFolder}/zig-cache/bin",
            "environment": [],
            "externalConsole": false,
            "logging": {
                "moduleLoad": false
            }
    ]
}